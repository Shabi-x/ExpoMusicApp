{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"jsx": "react-native",
		"lib": ["dom", "ESNext"],
		"noEmit": true,
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@types/*": ["./src/types/*"],
			"@assets/*": ["./src/assets/*"]
		}
	},
	"exclude": ["node_modules"]
}
